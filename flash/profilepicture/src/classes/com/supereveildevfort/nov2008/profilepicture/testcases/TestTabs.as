package com.supereveildevfort.nov2008.profilepicture.testcases {	import flash.events.Event;			import com.supereveildevfort.nov2008.profilepicture.data.ImageData;			import br.com.stimuli.loading.BulkErrorEvent;		import br.com.stimuli.loading.BulkProgressEvent;		import br.com.stimuli.loading.BulkLoader;			import com.supereveildevfort.nov2008.profilepicture.ui.TabWindow;			import flash.display.Sprite; 		/**	 * @class TestTabs.as	 * @namespace com.supereveildevfort.nov2008.profilepicture.testcases	 * @author Niqui Merret	 * @version 1.0	 * @date Nov 24, 2008	 * @description	 * @usage	 * NOTE:	 * TODO:	 *	 */	public class TestTabs extends Sprite 	{		private var mTabs : TabWindow;		private var mBulkLoader : BulkLoader;		public function TestTabs ()		{						mBulkLoader = new BulkLoader("TestTabs");            mBulkLoader.add("../../xml/profileimages.xml");           //mBulkLoader.add("/faces/profile-images.xml");        	mBulkLoader.addEventListener(BulkProgressEvent.COMPLETE, onAllItemsLoaded);        	mBulkLoader.addEventListener(BulkErrorEvent.ERROR, onLoadError);    		mBulkLoader.start();    		    				}				private function onAllItemsLoaded (e : BulkProgressEvent) : void		{						trace("XML loaded");			//parse XML						var imageXML : XML = mBulkLoader.getXML("../../xml/profileimages.xml");			//var imageXML : XML = mBulkLoader.getXML("/faces/images.xml");			ImageData.getInstance().parseXML(imageXML);			ImageData.getInstance().addEventListener(Event.COMPLETE, onLoadingComplete);		}				private function onLoadingComplete (e : Event) : void		{			mTabs = new TabWindow(ImageData.getInstance().profileImages);			addChild(mTabs);						// trace(ImageData.getInstance().profileImages.faceareacategories[0].faceareas[0].faceparts);		}		private function onLoadError (event : BulkErrorEvent) : void		{		}	}}