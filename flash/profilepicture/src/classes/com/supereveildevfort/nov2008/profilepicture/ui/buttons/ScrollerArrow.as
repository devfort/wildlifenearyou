package com.supereveildevfort.nov2008.profilepicture.ui.buttons {	import flash.display.Sprite;		import flash.utils.getDefinitionByName;			import com.niquimerret.accessibility.components.AccessButton; 		/**	 * @class ScrollerArrow.as	 * @namespace com.supereveildevfort.nov2008.profilepicture.ui.buttons	 * @author Niqui Merret	 * @version 1.0	 * @date Nov 24, 2008	 * @description	 * @usage	 * NOTE:	 * TODO:	 *	 */	public class ScrollerArrow extends AccessButton 	{				public function ScrollerArrow(pLable : String)		{						super(pLable);						}						override protected function init () : void		{			super.init();			var arrow : Sprite = new Sprite();			addChild(arrow);				var arrowC : Class = getDefinitionByName("Arrow") as Class;			var arrowImg : Sprite = new arrowC() as Sprite;			arrow.addChild(arrowImg);							}			}}