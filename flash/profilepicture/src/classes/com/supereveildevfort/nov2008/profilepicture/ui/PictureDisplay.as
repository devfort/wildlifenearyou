apData = pImage.bitmapData;		}						public function set nose(pImage : Bitmap) : void		{			var img : Bitmap = new Bitmap();			mNose.bitmapData = pImage.bitmapData;		}						public function set eye(pImage : Bitmap) : void		{			var img : Bitmap = new Bitmap();			mEyes.bitmapData = pImage.bitmapData;		}						public function set hair(pImage : Bitmap) : void		{			var img : Bitmap = new Bitmap();			mHair.bitmapData = pImage.bitmapData;		}						public function set hairAccessory(pImage : Bitmap) : void		{			var img : Bitmap = new Bitmap();			mHariAccessory.bitmapData = pImage.bitmapData;		}						public function set facialHair(pImage : Bitmap) : void		{			var img : Bitmap = new Bitmap();			mFacialHair.bitmapData = pImage.bitmapData;		}		/**		 *********************************************************************************************************************************************************		 * 		 * private		 * 		 *********************************************************************************************************************************************************		 */						private function init() : void		{						mFace = new Bitmap();			addChild(mFace);						mEars  = new Bitmap();			addChild(mEars);						mCheeks = new Bitmap();			addChild(mCheeks);						mMouth = new Bitmap();			addChild(mMouth);						mNose = new Bitmap();			addChild(mNose);						mEyes = new Bitmap();			addChild(mEyes);						mHair = new Bitmap();			addChild(mHair);						mHariAccessory = new Bitmap();			addChild(mHariAccessory);						mFacialHair = new Bitmap();			addChild(mFacialHair);					}					}}