package com.supereveildevfort.nov2008.profilepicture.testcases {	import flash.text.TextField;			import com.supereveildevfort.nov2008.profilepicture.ui.ElementsPanel;			import flash.events.Event;		import br.com.stimuli.loading.BulkErrorEvent;			import com.supereveildevfort.nov2008.profilepicture.data.ImageData;			import br.com.stimuli.loading.BulkProgressEvent;		import br.com.stimuli.loading.BulkLoader;			import flash.display.Sprite;		/**	 * @class TestImageData.as	 * @namespace com.supereveildevfort.nov2008.profilepicture.testcases.TestImageData	 * @author Niqui Merret	 * @version 1.0	 * @date Nov 24, 2008	 * @description	 * @usage	 * NOTE:	 * TODO:	 *	 */	public class TestImageData extends Sprite 	{		private var mBulkLoader : BulkLoader;				public var debug : TextField;		public function TestImageData () 		{						trace("loading XML");			debug.text = " Loading XML";						mBulkLoader = new BulkLoader("testImageData");            //mBulkLoader.add("../../xml/profileimages.xml");            mBulkLoader.add("/faces/profile-images.xml");        	mBulkLoader.addEventListener(BulkProgressEvent.COMPLETE, onAllItemsLoaded);        	mBulkLoader.addEventListener(BulkErrorEvent.ERROR, onLoadError);    		mBulkLoader.start();    				}				private function onAllItemsLoaded (e : BulkProgressEvent) : void		{						trace("XML loaded");			debug.text = "XML loaded";						//parse XML						//var imageXML : XML = mBulkLoader.getXML("../../xml/profileimages.xml");			var imageXML : XML = mBulkLoader.getXML("/faces/profile-images.xml");			ImageData.getInstance().parseXML(imageXML);			ImageData.getInstance().addEventListener(Event.COMPLETE, onLoadingComplete);		}				private function onLoadingComplete (e : Event) : void		{			debug.text = "XML images loaded";			var elementsPanel : ElementsPanel = new ElementsPanel();			elementsPanel.setUp(ImageData.getInstance().profileImages.faceareacategories[0].faceareas[0].faceparts);			addChild(elementsPanel);			elementsPanel.y = 400;									// trace(ImageData.getInstance().profileImages.faceareacategories[0].faceareas[0].faceparts);		}		private function onLoadError (e : BulkErrorEvent) : void		{						trace(e.errors);						//debug.text = e.errors;					}				 	}}