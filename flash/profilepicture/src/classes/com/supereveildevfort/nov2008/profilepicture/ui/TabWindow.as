package com.supereveildevfort.nov2008.profilepicture.ui {	import com.niquimerret.events.InteractionEvent;		import com.supereveildevfort.nov2008.profilepicture.ui.buttons.TabButton;			import flash.display.Sprite;import flash.utils.getDefinitionByName; 		/**	 * @class TabWindow.as	 * @namespace com.supereveildevfort.nov2008.profilepicture.ui	 * @author Niqui Merret	 * @version 1.0	 * @date Nov 25, 2008	 * @description	 * @usage	 * NOTE:	 * TODO:	 *	 */	public class TabWindow extends Sprite 	{		private var mFaceTab : TabButton;		private var mHairTab : TabButton;		private var mAccessoriesTab : TabButton;		private var mTabBG : Sprite;		public function TabWindow ()		{						init();		}				private function onTabClicked (e : InteractionEvent) : void		{						if (e.currentTarget != mFaceTab)			{				mFaceTab.active = true;			}			if (e.currentTarget != mHairTab)			{				mHairTab.active = true;			}			if (e.currentTarget != mAccessoriesTab)			{				mAccessoriesTab.active = true;			}		}				private function init() : void		{						mTabBG = new Sprite();			addChild(mTabBG);						var tabBGC : Class = getDefinitionByName("tabbg") as Class;			var tabBG : Sprite =  new tabBGC() as Sprite;			mTabBG.addChild(tabBG);						mFaceTab = new TabButton("Face");			addChild(mFaceTab);			mFaceTab.addEventListener(InteractionEvent.CLICKED, onTabClicked);						mHairTab = new TabButton("Hair");			addChild(mHairTab);			mHairTab.addEventListener(InteractionEvent.CLICKED, onTabClicked);						mAccessoriesTab = new TabButton("Accessories");			addChild(mAccessoriesTab);			mAccessoriesTab.addEventListener(InteractionEvent.CLICKED, onTabClicked);						mFaceTab.active = false;			mFaceTab.x = 15;			mHairTab.x = mFaceTab.width + mFaceTab.x + 5;			mAccessoriesTab.x = mHairTab.width + mHairTab.x + 5;							mTabBG.y = mFaceTab.height - 3;					}	}}