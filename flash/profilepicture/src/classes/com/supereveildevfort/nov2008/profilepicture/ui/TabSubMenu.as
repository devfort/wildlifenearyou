package com.supereveildevfort.nov2008.profilepicture.ui {	import com.niquimerret.events.InteractionEvent;		import com.supereveildevfort.nov2008.profilepicture.ui.buttons.SubTabButton;			import flash.display.Sprite; 		/**	 * @class TabSubMenu.as	 * @namespace com.supereveildevfort.nov2008.profilepicture.ui	 * @author Niqui Merret	 * @version 1.0	 * @date Nov 26, 2008	 * @description	 * @usage	 * NOTE:	 * TODO:	 *	 */	public class TabSubMenu extends Sprite 	{		private var mFaceAreas : Array;		private var mTabList : Array;		public function TabSubMenu () 		{			mTabList = new Array();		}				public function setMenuItems (pFaceAreas : Array) : void		{			mFaceAreas = pFaceAreas;						var i : uint;			var subButton : SubTabButton;									for (i = 0; i < mTabList.length; i++) 			{								mTabList[i].removeEventListener(InteractionEvent.CLICKED, onSubTabClicked);				removeChild(mTabList[i]);			}						mTabList = [];						for (i = 0; i < mFaceAreas.length; i++)			{				subButton = new SubTabButton(mFaceAreas[i].name);				addChild(subButton);								subButton.addEventListener(InteractionEvent.CLICKED, onSubTabClicked);				subButton.x = (i == 0) ? 15 : (mTabList[i-1].width + mTabList[i-1].x + 10);				mTabList.push(subButton);			}		}				private function onSubTabClicked (e : InteractionEvent) : void		{		}	}}